package vector;

public class Vector {

	Double x = null;
	Double y = null;
	Double z = null;
	Double magnitude = null;
	Double angle = null;
	
	public Vector(Dimensions dimension, Double magnitude, Double angle) {
		
	}
	Vector(Dimensions dimension, Double x, Double y, Double z) {
		switch(dimension) {
		case THREEDIM:
			this.z = z;
		case TWODIM:
			this.y = y;
		case ONEDIM:
			this.x = x;
			break;
		default:
			break;
		}
	}
}
