package main;

public class VecMain {

	public static void main(String[] args) {
		System.out.println("Welcome to Vector Land!");
		String choice = "0";
		boolean exit = false;
		do {
			System.out.println("How may we serve you?");
			
			switch(choice) {
			case "0":
				exit = true;
				break;
			}
		} while (!exit);
		
		System.out.println("Completed...");
	}
}
