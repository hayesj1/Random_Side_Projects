package vector;

public class VectorFactory {
	
	public VectorFactory(Integer numDims) {
		if (numDims == null) {
			
		}
		switch(numDims) {
		case 3:
			i = new Vector(numDims, 1.0, 0.0, 0.0);
			j = new Vector(numDims, 0.0, 1.0, 0.0);
			k = new Vector(numDims, 0.0, 0.0, 1.0);
			break;
		case 2:
			i = new Vector(numDims, 1.0, 0.0, null);
			j = new Vector(numDims, 0.0, 1.0, null);
			break;
		case 1:
			i = new Vector(numDims, 1.0, null, null);
			break;
		default:
			i = null;
			j = null;
			k = null;
			break;
		}
	}
}
